a = 0;
c = 0;

while(a < 20){
    a++;
    if(a==10) continue;
    if(a==15) continue;
    b = 10;
    while(b-->0){
        c += b;
        if(b == 5) continue;
    }
    c = c + a;
}
assert(c==995);